@page
@model PrivacyModel
@{
    ViewData["Title"] = "My Cart";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>
<div>
    <section>
        <table id="stockTable" class="table table-striped table-bordered cell-border stripe" style="width:100%">
            <thead>
                <tr>
                    <th>Item Name</th>
                    <th>Quantity</th>
                    <th>Price</th>
                    <th>Photo</th>
                    <th>IsCheckedOut</th>
                </tr>
            </thead>
            <tbody>

                <tr>
                    <td>Apple AirPods</td>
                    <td>1</td>
                    <td>Body 4</td>
                    <td>
                        @DateTime.Now.ToString("yyyy/MM/dd HH:mm:ss tt")
                    </td>
                    <td>R 4300.30</td>
                    <td>False</td>
                </tr>
            </tbody>
        </table>
    </section>
</div>